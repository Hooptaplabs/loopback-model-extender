{
  "name": "loopback-model-extender",
  "version": "1.0.0",
  "description": "A json centered loopback model extender",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir tmp-babel && node ./scripts/uglify && rimraf tmp-babel",
    "eslint": "eslint ./src",
    "test": "jasmine-node ./spec --color",
    "test-watch": "jasmine-node ./spec/ --autotest --watch ./src --color",
    "coverage": "istanbul cover -x '**/spec/**' jasmine-node --captureExceptions ./spec/ --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hooptaplabs/loopback-model-extender.git"
  },
  "author": "Roger Fos Soler (desaroger23@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hooptaplabs/loopback-model-extender/issues"
  },
  "homepage": "https://github.com/Hooptaplabs/loopback-model-extender#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "codeclimate-test-reporter": "^0.3.1",
    "eslint": "^2.10.2",
    "eslint-config-loopback": "^3.0.1",
    "istanbul": "^0.4.3",
    "jasmine-node": "^1.14.5",
    "lodash": "^4.13.1",
    "node-uglifier": "^0.4.5",
    "nodemon": "^1.9.2",
    "rimraf": "^2.5.2"
  }
}
