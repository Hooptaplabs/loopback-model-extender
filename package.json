{
  "name": "loopback-model-extender",
  "version": "1.0.0",
  "description": "A json centered loopback model extender",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir tmp-babel && node ./scripts/uglify && ./node_modules/.bin/rimraf tmp-babel",
    "eslint": "./node_modules/.bin/eslint ./src; exit 0",
    "test": "./node_modules/.bin/jasmine-node ./spec/ --color",
    "test-watch": "./node_modules/.bin/jasmine-node --autotest ./spec/ --color",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/jasmine-node --captureExceptions ./spec/ --color && cat ./coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hooptaplabs/loopback-model-extender.git"
  },
  "author": "Roger Fos Soler (desaroger23@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hooptaplabs/loopback-model-extender/issues"
  },
  "homepage": "https://github.com/Hooptaplabs/loopback-model-extender#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "eslint-config-loopback": "^3.0.1",
    "istanbul": "^0.4.3",
    "jasmine-node": "^1.14.5",
    "node-uglifier": "^0.4.5",
    "rimraf": "^2.5.2"
  }
}
